{
  "name": "victorsamuel-website",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "-------------------------------------------------": "",
    "prebuild": "rimraf dist/",
    "build": "npm run build:render",
    "build:ssr": "npm run build:aot && npm run build:server",
    "build:aot": "npm run build:aot:en && npm run build:aot:es",
    "build:aot:en": "ng build --configuration=production-en --base-href=/en/ --deploy-url=/en/",
    "build:aot:es": "ng build --configuration=production-es --base-href=/es/ --deploy-url=/es/",
    "postbuild:aot": "npm run resolve-assets",
    "build:server": "npm run build:server:en && npm run build:server:es",
    "build:server:en": "ng run victorsamuel-website:server:production-en",
    "build:server:es": "ng run victorsamuel-website:server:production-es",
    "build:render": "npm run build:render:en && npm run build:render:es",
    "build:render:en": "ng run victorsamuel-website:prerender:production-en --routesFile src/static.paths.txt",
    "build:render:es": "ng run victorsamuel-website:prerender:production-es --routesFile src/static.paths.txt",
    "postbuild:render": "npm run resolve-assets && npm run resolve-lang-manager",
    "--------------------------------------------------": "",
    "serve": "npm run serve:aot:en",
    "serve:aot:en": "ng serve --configuration=development-en --port 4200",
    "serve:aot:es": "ng serve --configuration=development-es --port 4201",
    "serve:ssr:en": "node dist/server/en/main.js",
    "serve:ssr:es": "node dist/server/es/main.js",
    "serve:firebase": "firebase serve",
    "---------------------------------------------------": "",
    "dev:ssr:en": "ng run victorsamuel-website:serve-ssr:development-en",
    "dev:ssr:es": "ng run victorsamuel-website:serve-ssr:development-es",
    "----------------------------------------------------": "",
    "deploy": "npm run deploy:hosting",
    "deploy:hosting": "firebase deploy --only hosting",
    "-----------------------------------------------------": "",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "------------------------------------------------------": "",
    "i18n:extract": "ng xi18n --output-path locale",
    "resolve-assets": "cpr dist/browser/en/assets/ dist/browser/assets/ && rimraf dist/browser/en/assets/",
    "resolve-lang-manager": "cpr src/index.lang.html dist/browser/index.html",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module",
      "options": {}
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.2",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/fire": "^6.1.1",
    "@angular/forms": "^11.0.2",
    "@angular/localize": "^11.0.2",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/platform-server": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@ngrx/store": "^10.0.1",
    "@nguniversal/express-engine": "^11.0.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^2.6.11",
    "express": "^4.15.2",
    "firebase": "^7.24.0",
    "intersection-observer": "^0.10.0",
    "ng-in-viewport": "^6.1.4",
    "npm": "^6.14.9",
    "object-fit-images": "^3.2.4",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "vanilla-tilt": "^1.7.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@angular/language-service": "^11.0.2",
    "@nguniversal/builders": "^11.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.9",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.13.32",
    "@types/object-fit-images": "^3.2.0",
    "codelyzer": "^6.0.0",
    "cpr": "^3.0.1",
    "firebase-tools": "^8.16.2",
    "fuzzy": "^0.1.3",
    "import-sort-parser-typescript": "^6.0.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.5"
  }
}
