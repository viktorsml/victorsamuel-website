{
  "name": "victorsamuel-website",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "-------------------------------------------------": "",
    "build": "npm run build:ssr",
    "build:ssr": "npm run build:aot && npm run build:server",
    "build:aot": "npm run build:aot:en && npm run build:aot:es",
    "build:aot:en": "ng build --configuration=production-en --base-href=/en/ --deploy-url=/en/",
    "build:aot:es": "ng build --configuration=production-es --base-href=/es/ --deploy-url=/es/",
    "postbuild:aot": "npm run resolve-assets",
    "build:server": "npm run build:server:en && npm run build:server:es",
    "build:server:en": "ng run victorsamuel-website:server:production-en",
    "build:server:es": "ng run victorsamuel-website:server:production-es",
    "postbuild:server": "npm run build:functions",
    "build:functions": "cd functions/ && npm run build",
    "build:render": "npm run build:render:en && npm run build:render:es",
    "build:render:en": "ng run victorsamuel-website:prerender:production-en --routesFile src/static.paths.txt",
    "build:render:es": "ng run victorsamuel-website:prerender:production-es --routesFile src/static.paths.txt",
    "postbuild:render": "npm run resolve-assets && npm run resolve-lang-manager",
    "--------------------------------------------------": "",
    "serve": "npm run serve:aot:en",
    "serve:aot:en": "ng serve --configuration=development-en --port 4200",
    "serve:aot:es": "ng serve --configuration=development-es --port 4201",
    "serve:ssr:en": "node dist/server/en/main.js",
    "serve:ssr:es": "node dist/server/es/main.js",
    "serve:firebase": "firebase serve",
    "serve:functions": "cd functions/ && npm run serve",
    "---------------------------------------------------": "",
    "dev:ssr:en": "ng run victorsamuel-website:serve-ssr:development-en",
    "dev:ssr:es": "ng run victorsamuel-website:serve-ssr:development-es",
    "----------------------------------------------------": "",
    "deploy": "firebase deploy",
    "deploy:hosting": "npm run build:ssr && firebase deploy --only hosting",
    "deploy:functions": "cd functions/ && npm run deploy",
    "-----------------------------------------------------": "",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "------------------------------------------------------": "",
    "i18n:extract": "ng xi18n --output-path locale",
    "resolve-assets": "cp -r dist/browser/en/assets/ dist/browser/ && rm -r dist/browser/en/assets/",
    "resolve-lang-manager": "cp src/index.lang.html dist/browser/index.html",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module",
      "options": {}
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/cdk": "~9.2.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "~9.1.1",
    "@angular/localize": "^9.1.1",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/platform-server": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@ngrx/store": "^9.1.2",
    "@nguniversal/express-engine": "^9.1.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^2.6.11",
    "express": "^4.15.2",
    "firebase": "^7.14.0",
    "intersection-observer": "^0.10.0",
    "ng-in-viewport": "^6.1.0",
    "npm": "^6.14.4",
    "object-fit-images": "^3.2.4",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.1",
    "vanilla-tilt": "^1.7.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "^9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@nguniversal/builders": "^9.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.11.1",
    "@types/object-fit-images": "^3.2.0",
    "codelyzer": "^5.2.2",
    "firebase-tools": "^8.0.3",
    "fuzzy": "^0.1.3",
    "import-sort-parser-typescript": "^6.0.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  }
}
